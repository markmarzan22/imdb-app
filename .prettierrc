{
    "semi": true,
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "importOrder": [
        "^react$",
        "<THIRD_PARTY_MODULES>",
        "^config",
        "^utils",
        "^view"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderParserPlugins": ["jsx", "typescript", "classProperties"]
}
